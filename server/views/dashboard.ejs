
    <div class="container ">
        <div class="row">
            <div class="col">
                <h2>User Data</h2>
            </div>
            <div class="col d-flex justify-content-end">
                <a href="/dashboard/create" class="btn btn-primary">Add user</a>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <table class="table">
                    <thead>
                      <tr>
                        <th scope="col">No</th>
                        <th scope="col">Username</th>
                        <th scope="col">Email</th>
                        <th scope="col">Action</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% players.forEach((player, index) => { %>
                      <tr>
                        <th scope="row"><%= index + 1 %></th>
                        <td><%= player.username %></td>
                        <td><%= player.email %></td>
                        <td>
                          <a class="btn btn-info btn-sm" href="/dashboard/detail/<%= player.id %>">Detail</a>
                          <a class="btn btn-danger btn-sm" href="/dashboard/delete/<%= player.id %>">Delete</a>
                        </td>
                      </tr>
                      <% }) %>
                    </tbody>
                  </table>
            </div>
        </div>
    </div>
