
    <div class="container ">
        <div class="row">
            <div class="col">
                <h2>User Data</h2>
            </div>
            <div class="col d-flex justify-content-end">
                <a href="/dashboard/create" class="btn btn-primary">Add user</a>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div class="row">
                    <div class="col">
                        Username
                    </div>
                    <div class="col">
                        <%= playerData.username %>
                    </div>
                </div>
                <div class="row">
                    <div class="col">
                        Email
                    </div>
                    <div class="col">
                        <%= playerData.email %>
                    </div>
                </div>
                <div class="row">
                    <div class="col">
                        Experience
                    </div>
                    <div class="col">
                        <%= playerData.experience %>
                    </div>
                </div>
                <div class="row">
                    <div class="col">
                        Level
                    </div>
                    <div class="col">
                        <%= playerData.lvl %>
                    </div>
                </div>
            </div>
        </div>
    </div>
